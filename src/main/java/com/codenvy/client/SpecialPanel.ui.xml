<!DOCTYPE ui:UiBinder SYSTEM
        "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.google.gwt.user.cellview.client">

    <ui:with field="res" type="com.codenvy.client.Resourse"/>

    <ui:style>

        .buttonPanel {
            margin: 20px;
        }
        .table {
            margin: 20px 50px;
        }
    </ui:style>

    <g:DockLayoutPanel unit="EM" width="500px" height="300px">

        <g:east size="20">
            <g:FlowPanel width="80px" addStyleNames="{style.buttonPanel} ">
                <g:Button height="30px" width="60px" ui:field="first" text="first" addStyleNames="{res.css.button}"/>
                <g:Button height="30px" width="60px" ui:field="second" text="second" addStyleNames="{res.css.button}"/>
                <g:Button height="30px" width="60px" ui:field="third" text="third" addStyleNames="{res.css.button}"/>
            </g:FlowPanel>
        </g:east>

        <g:center>
            <c:CellTable ui:field="table" addStyleNames="{style.table}"/>
        </g:center>

    </g:DockLayoutPanel>

</ui:UiBinder>